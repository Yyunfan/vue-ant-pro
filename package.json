{
  "name": "ant-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "container-query-toolkit": "^0.1.3",
    "cross-env": "^5.2.0",
    "dayjs": "^1.6.9",
    "echarts": "^4.1.0",
    "enquire.js": "^2.1.6",
    "express-http-proxy": "^1.2.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.9.0",
    "numeral": "^2.0.6",
    "qs": "^6.5.2",
    "resize-observer-lite": "^0.2.3",
    "sass-loader": "^7.0.2",
    "url": "^0.11.0",
    "url-polyfill": "^1.0.13",
    "vue": "^2.5.16",
    "vue-antd-ui": "^0.6.7",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "babel-plugin-add-module-exports": "^0.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "no-extra-semi": 0,
      "indent": [
        "error",
        4
      ],
      "semi": 0,
      "no-unused-vars": 0,
      "space-before-function-paren": 0,
      "comma-dangle": 0
    },
    "globals": {
      "FormData": false,
      "localStorage": false
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
